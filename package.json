{
  "name": "debt-collection-orchestration-engine",
  "version": "1.0.0",
  "description": "A simplified backend version of a `Debt Collection Orchestration Engine (DCOE)` for Esolve Group",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:create": "npx sequelize-cli db:create --config src/config/sequelize-cli-config.js",
    "db:migrate": "npx sequelize-cli db:migrate --config src/config/sequelize-cli-config.js --migrations-path src/migrations",
    "db:migrate:down": "npx sequelize-cli db:migrate:undo --config src/config/sequelize-cli-config.js --migrations-path src/migrations",
    "db:seeders": "npx sequelize-cli db:seed:all --seeders-path src/seeders --config src/config/sequelize-cli-config.js",
    "db:seeders:down": "npx sequelize-cli db:seed:undo --seeders-path src/seeders --config src/config/sequelize-cli-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asifsk-dev/debt-collection-orchestration-engine.git"
  },
  "author": "Asif Sk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asifsk-dev/debt-collection-orchestration-engine/issues"
  },
  "homepage": "https://github.com/asifsk-dev/debt-collection-orchestration-engine#readme",
  "resolutions": {
    "@types/express": "4.17.23",
    "@types/express-serve-static-core": "4.17.41"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-validation": "^4.1.0",
    "fast-csv": "^5.0.5",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.2",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-ui-express": "^4.1.8",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
